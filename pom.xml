            </dependency>

            <dependency>
                <groupId>org.scala-lang</groupId>
                <artifactId>scala-library</artifactId>
                <version>${scala.version}</version>
            </dependency>
			
            <dependency>
                <groupId>de.fuberlin.wiwiss.silk</groupId>
                <artifactId>silk-core</artifactId>
                <version>${silk.version}</version>
            </dependency>
            <dependency>
